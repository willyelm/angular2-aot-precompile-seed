{
  "name": "angular2-aot-seed",
  "version": "0.0.1",
  "description": "A Simple Angular 2 seed supporting Bootstrap, SASS and Pug compilation.",
  "scripts": {
    "prebuild": "npm run clean && npm run pug && npm run sass && npm run ngc",
    "build": "NODE_ENV='production' webpack --progress",
    "postbuild": "npm run clean",
    "ngc": "node_modules/.bin/ngc -p ./tsconfig.aot.json",
    "sass": "node-sass src/app -o src/app --include-path node_modules/bootstrap/scss --output-style compressed",
    "pug": "pug src/app --out src/app",
    "start": "NODE_ENV='development' webpack-dev-server --progress --hot",
    "clean": "rm -Rf src/app/**/*.html && rm -Rf src/app/**/*.css && rm -Rf build"
  },
  "contributors": [
    "Williams Medina <williams.medinaa@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.4.6",
    "@angular/compiler": "~2.4.6",
    "@angular/compiler-cli": "~2.4.6",
    "@angular/core": "~2.4.6",
    "@angular/forms": "~2.4.6",
    "@angular/http": "~2.4.6",
    "@angular/platform-browser": "~2.4.6",
    "@angular/platform-browser-dynamic": "~2.4.6",
    "@angular/platform-server": "~2.4.6",
    "@angular/router": "^3.4.6",
    "@angular/upgrade": "~2.4.6",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.20",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "ie-shim": "^0.1.0",
    "node-sass": "^4.5.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.3",
    "zone.js": "~0.7.5"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.2.8",
    "@types/node": "^6.0.48",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.25.0",
    "extension-replace-loader": "0.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "pug-cli": "^1.0.0-alpha6",
    "pug-html-loader": "^1.1.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^5.0.0",
    "style-loader": "^0.13.1",
    "typescript": "~2.0.10",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
